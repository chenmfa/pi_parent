<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pi.stroop.dao.mapper.StroopDiagnosisHistoryMapper">
	<sql id="all_columns">
		id,create_date,update_date,version,user_id,invite_id,invite_user_id,diagnosis_info,diagnosis_result,diagnosis_score,diagnosis_level
	</sql>
	<sql id="updateSet">
		<if test="userId != null ">,user_id=#{userId}</if>
		<if test="inviteId != null ">,invite_id=#{inviteId}</if>
		<if test="inviteUserId != null ">,invite_user_id=#{inviteUserId}</if>
		<if test="diagnosisInfo != null ">,diagnosis_info=#{diagnosisInfo}</if>
		<if test="diagnosisResult != null ">,diagnosis_result=#{diagnosisResult}</if>
		<if test="diagnosisScore != null ">,diagnosis_score=#{diagnosisScore}</if>
		<if test="diagnosisLevel != null ">,diagnosis_level=#{diagnosisLevel}</if>

	</sql>
	<sql id="where">
		<where>
			<if test="id != null ">and id=#{id}</if>
			<if test="createDate != null ">and create_date=#{createDate}</if>
			<if test="updateDate != null ">and update_date=#{updateDate}</if>
			<if test="version != null ">and version=#{version}</if>
			<if test="userId != null ">and user_id=#{userId}</if>
			<if test="inviteId != null ">and invite_id=#{inviteId}</if>
			<if test="inviteUserId != null ">and invite_user_id=#{inviteUserId}</if>
			<if test="diagnosisInfo != null ">and diagnosis_info=#{diagnosisInfo}</if>
			<if test="diagnosisResult != null ">and diagnosis_result=#{diagnosisResult}</if>
			<if test="diagnosisScore != null ">and diagnosis_score=#{diagnosisScore}</if>
			<if test="diagnosisLevel != null ">and diagnosis_level=#{diagnosisLevel}</if>
			
		</where>
	</sql>
	<sql id="table_name">
		stroop_diagnosis_history
	</sql>

	<select id="findOne" resultType="StroopDiagnosisHistoryEntity">
		select
		<include refid="all_columns" />
		from
		<include refid="table_name" />
		where id=#{id}
	</select>
  <select id="findAll" resultType="StroopDiagnosisHistoryEntity">
    SELECT
    <include refid="all_columns" />
    <include refid="table_name" /> limit 1000
  </select>
  <select id="findList" resultType="StroopDiagnosisHistoryEntity">
    select <include refid="all_columns" /> from <include refid="table_name" /> <include refid="where" />
  </select>
  
    
	<update id="updateById" parameterType="com.pi.stroop.dao.entity.StroopDiagnosisHistoryEntity">
		update <include refid="table_name" />
		set version=version+1, update_date=now()  <include refid="updateSet" />
		where ID=#{id} AND VERSION=#{version}
	</update>
	
	
	<insert id="insert" useGeneratedKeys="true" keyProperty="id"  keyColumn="id" parameterType="com.pi.stroop.dao.entity.StroopDiagnosisHistoryEntity">
		insert into <include refid="table_name" />
		(<include refid="all_columns" />)
		values( NULL, now(), now(), 200,#{userId},#{inviteId},#{inviteUserId},#{diagnosisInfo},#{diagnosisResult},#{diagnosisScore},#{diagnosisLevel} )
	</insert>

  <select id="count" resultType="long">
    SELECT count(1) FROM <include refid="table_name" />
  </select>
  <select id="countList" resultType="long">
    SELECT count(1) FROM <include refid="table_name" />
    <include refid="where" />
  </select>	
  <select id="getByIds" parameterType="list" resultType="StroopDiagnosisHistoryEntity">
    SELECT
    <include refid="all_columns" />
    FROM <include refid="table_name" />
    <where>
      ID in
      <foreach item="item" index="index" collection="list" open="("
           separator="," close=")">
        #{item}
      </foreach>
    </where>
  </select>
  
	<delete id="delete" parameterType="long">
	  DELETE FROM <include refid="table_name" />
	  WHERE ID=#{id}
	</delete>
  <delete id="deleteByIds">
    DELETE FROM <include refid="table_name" />
    <where>
      ID in
      <foreach item="item" index="index" collection="list" open="("
           separator="," close=")">
        #{item}
      </foreach>
    </where>
  </delete>
</mapper>